367  VALID perfect Square
brute force:

class Solution {
    public boolean isPerfectSquare(int num) {
        for(int i=1;i<=num;i++){
            if((i*i)==num){
                return true;
            }
        }
        return false;
        
    }
}


class Solution {
    public boolean isPerfectSquare(int num) {
        if(num==1){
            return true;
            
        }
        for(int i=2;i<=num/2;i++){
            if((i*i)==num){
                return true;
            }
        }
        return false;
        
    }
}
