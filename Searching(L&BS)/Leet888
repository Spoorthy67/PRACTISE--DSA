Brute force:

class Solution {
    public int[] fairCandySwap(int[] aliceSizes, int[] bobSizes) {
        int[] ans=new int[2];
        int totalCandiesAlice=0;
        int totalCandiesbob=0;
        for(int i=0;i<aliceSizes.length;i++){
            totalCandiesAlice+=aliceSizes[i];
        }
        for(int j=0;j<bobSizes.length;j++){
            totalCandiesbob+=bobSizes[j];
        }
        for(int i=0;i<aliceSizes.length;i++){
            for(int j=0;j<bobSizes.length;j++){
                if((totalCandiesAlice-aliceSizes[i]+bobSizes[j])==(totalCandiesbob-bobSizes[j]+aliceSizes[i])){
                    ans[0]=aliceSizes[i];
                    ans[1]=bobSizes[j];
                }
            }
        }
        return ans;


        
    }
}
